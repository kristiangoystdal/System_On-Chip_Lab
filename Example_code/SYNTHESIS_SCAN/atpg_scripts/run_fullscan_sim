#!/bin/ksh
 
# Template script for running Cadence Incisive simulator (NCSIM) on Cadence Encounter Test generated test vectors.
 
#  Generated by Genus(TM) Synthesis Solution 19.14-s108_1
 
export WORKDIR=./atpg_scripts
 
xrun \
   +access+rwc \
   +xmstatus \
   +xm64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter4bit_scan_atpg.data.scan.ex1.ts1.verilog \
   +TESTFILE2=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter4bit_scan_atpg.data.logic.ex1.ts2.verilog \
   +HEARTBEAT \
   +FAILSET \
   +xmtimescale+1ns/1ps \
   +xmoverride_timescale \
   +xmseq_udp_delay+2ps \
   +libext+.v+.V+.z+.Z+.gz \
   +xmlibdirname+$WORKDIR/Inca_libs_10_25_13 \
   -l $WORKDIR/ncverilog_FULLSCAN.log \
  -v ../../SIMULATION/verilog_libs/fsc0h_d_generic_core_21.lib.src \
$WORKDIR/counter4bit_scan.test_netlist.v \
$WORKDIR/testresults/verilog/VER.FULLSCAN.counter4bit_scan_atpg.mainsim.v

