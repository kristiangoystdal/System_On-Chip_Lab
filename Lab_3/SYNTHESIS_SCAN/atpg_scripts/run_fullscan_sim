export WORKDIR=./atpg_scripts
 
xrun \
   +access+rwc \
   +xmstatus \
   +xm64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.BATCHARGER_controller_atpg.data.scan.ex1.ts1.verilog \
